<!DOCTYPE html>
<html lang="en">
  <%- include('partials/scripts.ejs') %>
  <body>
    <%- include('partials/header.ejs') %>
    <div class="container">
      <div class="row">
        <div class="col">
          <table class="table table-striped">
            <tr>
              <th>Firstname</th>
              <th>Email</th>
              <th>Phone</th>
              <th>Compnay Name</th>
            </tr>

            <% for(let i=0; i < users.length; i++) { 
                const user = users[i]
                const company = users[i].company
            %>
            <tr>
              <td><%= user.name %></td>
              <td><%= user.email %></td>
              <td><%= user.phone %></td>
              <td><%= company.name %></td>
            </tr>
            <% } %>
          </table>
        </div>
      </div>
    </div>
  </body>
</html>